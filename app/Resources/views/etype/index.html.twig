{% extends 'base.html.twig' %}

{% block body %}
    <h1>Etypes list</h1>
 
    <table class="table table-striped table-bordered " id="table" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>Id</th>
                <th>Etype</th>
                <th>Validityperiod</th>
                <th>Eprice</th>
                <th>Producerid</th>
                <th>Laboratoryid</th>
                <th>Verificationid</th>
                <th>Emodelid</th>
                <th class="hidden-xs">Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etype in etypes %}
            <tr>
                <td><a href="{{ path('etype_show', { 'id': etype.id }) }}">{{ etype.id }}</a></td>
                <td>{{ etype.eType }}</td>
                <td>{{ etype.validityPeriod }}</td>
                <td>{{ etype.ePrice }}</td>
                <td>{{ etype.producer }}</td>
                <td>{{ etype.laboratory }}</td>
                <td>{{ etype.verification }}</td>
                <td>{{ etype.emodel }}</td>
                <td class="hidden-xs">
                    <div class="btn-group">
                            <a href="{{ path('etype_edit', { 'id': etype.id }) }}" class="btn-sm btn-primary" role="button">edit</a>
                            <a href="{{ path('etype_edit', { 'id': etype.id }) }}" class="btn-sm btn-primary" role="button">edit</a>
                    </div>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


    <ul>
        <li>
            <a href="{{ path('etype_new') }}">Create a new etype</a>
        </li>
    </ul>

    
{% endblock %}
